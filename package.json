{
  "name": "vue-ssr-benchmarks",
  "version": "1.0.0",
  "description": "benchmarks for ssr of vue apps",
  "scripts": {
    "clean:build": "rimraf ./dist",
    "clean:results": "rimraf ./results",
    "build": "npm run clean:build && cross-env NODE_ENV=production webpack -c ./webpack.config.js",
    "bench": "npm run clean:results && cross-env NODE_ENV=production node ./dist/index.js",
    "server": "cross-env NODE_ENV=production node ./dist/server.js"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.4.21",
    "fastify": "^4.26.2",
    "vue": "^3.4.21",
    "vue-loader": "^17.4.2"
  },
  "devDependencies": {
    "@vue/babel-plugin-jsx": "^1.2.2",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "glob": "^10.3.12",
    "markdown-table": "^3.0.3",
    "rimraf": "^5.0.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
